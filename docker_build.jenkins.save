#!groovy
// Run docker build
 
properties([disableConcurrentBuilds()])

pipeline {
    agent any 
    options {
        buildDiscarder(logRotator(numTKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages {
        stage("First step") {
            steps {
                sh 'ssh -o StrictHostKeyChecking=no root@myUbuntuSrv \'hostname\''
            }
        }
	stage("First step"){
	    steps{
               sh 'ssh -o StrictHostKeyChecking=no root@myUbuntuSrv \'uptime\''
	    }        
    }
}
